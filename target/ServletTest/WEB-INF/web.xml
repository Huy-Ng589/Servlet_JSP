<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

<!--  Starting servlet-->
  <servlet>
    <servlet-name>Servlets</servlet-name>
    <servlet-class>com.bkav.servlet.Servlets</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Servlets</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
<!--  End servlet 1-->

  <servlet>
    <servlet-name>Authentication</servlet-name>
    <servlet-class>com.bkav.servlet.Authentication</servlet-class>
  </servlet>

  <servlet-mapping>
  <servlet-name>Authentication</servlet-name>
  <url-pattern>/login</url-pattern>
  </servlet-mapping>
<!--  End servlet 2-->

  <servlet>
    <servlet-name>Bear Select</servlet-name>
    <servlet-class>com.bkav.servlet.BearSelect</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Bear Select</servlet-name>
    <url-pattern>/select</url-pattern>
  </servlet-mapping>
<!--  End servlet 3-->

  <servlet>
    <servlet-name>Characteristic</servlet-name>
    <servlet-class>com.bkav.servlet.SelectCharacteristic</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Characteristic</servlet-name>
    <url-pattern>/char</url-pattern>
  </servlet-mapping>
<!--  End servlet 4-->
<!--  End servlet-->



<!--  Starting JSP-->
  <servlet>
    <servlet-name>Sample JSP</servlet-name>
    <jsp-file>/process/index.jsp</jsp-file>
  </servlet>

  <servlet-mapping>
    <servlet-name>Sample JSP</servlet-name>
    <url-pattern>/jsp</url-pattern>
  </servlet-mapping>
<!--  End JSP-->

